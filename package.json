{
  "name": "notable-notary",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start:prod": "node dist/index.js",
    "start": "node --inspect=5858 -r ts-node/register -r dotenv/config ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "check": "npm run check:build && npm run check:prettier && npm run check:eslint",
    "check:build": "tsc --noEmit",
    "check:eslint": "eslint --ext .js,.ts",
    "check:prettier": "npm run fix:prettier -- --check",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix --ext .js,.ts",
    "fix:prettier": "prettier src/ test/",
    "test:unit": "jest",
    "test:integration": "jest --config jest.integration.config.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
